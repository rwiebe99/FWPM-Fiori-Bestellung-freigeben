<mvc:View
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="acceptpurchaseorder.controller.Master">
	<Page
		title="Orders List">
		<List id="orders" 
				items="{/ZOSO_PURCHASEORDER}"
				mode="SingleSelectMaster"
				selectionChange="onSelectionChange">

				<ObjectListItem
				title="{PurchaseOrder}"
				icon="sap-icon://product"
				type="Active"
				press="onListItemPress"
			>
			</ObjectListItem>
			
		</List>
	</Page>
</mvc:View>
