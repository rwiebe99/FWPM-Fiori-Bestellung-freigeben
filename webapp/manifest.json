{
	"sap.app": {
		"id": "acceptpurchaseorder.routingApp",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.98.0"
		},
		"rootView": {
			"viewName": "acceptpurchaseorder.view.App",
			"type": "XML",
			"async": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"path": "acceptpurchaseorder.view",
				"controlId": "rootControl",
				"type": "View",
				"viewType": "XML",
				"async": true
			},
			"routes": [
				{
					"name": "master",
					"pattern": "",
					"target": [
						"master"
					]
				},
				{
					"name": "orderDetails",
					"pattern": "orders/:orderId:",
					"target": [
						"master",
						"orderDetails"
					]
				}
				
			],
			"targets": {
				"master": {
					"name": "Master",
					"controlAggregation": "masterPages",
					"level": 0
				},
				"orderDetails": {
					"name": "Detail",
					"controlAggregation": "detailPages",
					"title": {
						"parts": [
							"orderName"
						],
						"formatter": "jQuery.sap.formatMessage"
					},
					"level": 1
				}
			}
		}
	}
}