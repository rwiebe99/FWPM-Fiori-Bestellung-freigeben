{
	"sap.app": {
		"id": "acceptpurchaseorder.routingApp",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"dataSources": {
		  "mainService": {
			"uri": "https://s4h.ososoft.de:44300/sap/opu/odata/sap/ZOSO_PURCHASEORDER/",
			"type": "OData",
			"settings": {
				"odataVersion": "2.0"
			}
			
		  }
		}
	
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.98.0"
		},
		"rootView": {
			"viewName": "acceptpurchaseorder.view.App",
			"type": "XML",
			"async": true
		},
		"models":{
			"invoice":{
				"dataSource": "mainService"

				}
			},
		
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"path": "acceptpurchaseorder.view",
				"controlId": "rootControl",
				"type": "View",
				"viewType": "XML",
				"async": true
			},
			"routes": [
				{
					"name": "master",
					"pattern": "",
					"target": [
						"master"
					]
				},
				{
					"name": "orderDetails",
					"pattern": "orders/:product:",
					"target": [
						"master",
						"orderDetails"
					]
				}
				
			],
			"targets": {
				"master": {
					"name": "Master",
					"controlAggregation": "masterPages",
					"level": 0
				},
				"orderDetails": {
					"name": "Detail",
					"controlAggregation": "detailPages",
					"title": {
						"parts": [
							"orderName"
						],
						"formatter": "jQuery.sap.formatMessage"
					},
					"level": 1
				}
			}
		}
	}
}