{
	"sap.app": {
		"id": "acceptpurchaseorder.routingApp",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"dataSources": {
			"mainService": {
				"uri": "https://s4h.ososoft.de:44300/sap/opu/odata/sap/ZOSO_PURCHASEORDER/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"buttonService": {
				"uri": "https://s4h.ososoft.de:44300/sap/opu/odata/SAP/ZOSO_PO_UTIL_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"i18n": {
				"bundleName": "acceptpurchaseorder.i18n.i18n",
				"supportedLocales": [
					"de",
					"en"
				],
				"fallbackLocale": "en"
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.98.0"
		},
		"rootView": {
			"viewName": "acceptpurchaseorder.view.App",
			"type": "XML",
			"async": true
		},
		"models": {
			"invoice": {
				"dataSource": "mainService"
			},
			"button": {
				"dataSource": "buttonService"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "acceptpurchaseorder.i18n.i18n",
					"supportedLocales": [
						"de",
						"en"
					],
					"fallbackLocale": "en"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"path": "acceptpurchaseorder.view",
				"controlId": "rootControl",
				"type": "View",
				"viewType": "XML",
				"async": true
			},
			"routes": [
				{
					"name": "master",
					"pattern": "",
					"target": [
						"master"
					]
				},
				{
					"name": "orderDetails",
					"pattern": "orders/:product:",
					"target": [
						"master",
						"orderDetails"
					]
				}
			],
			"targets": {
				"master": {
					"name": "Master",
					"controlAggregation": "masterPages",
					"level": 0
				},
				"orderDetails": {
					"name": "Detail",
					"controlAggregation": "detailPages",
					"title": {
						"parts": [
							"orderName"
						],
						"formatter": "jQuery.sap.formatMessage"
					},
					"level": 1
				}
			}
		}
	}
}